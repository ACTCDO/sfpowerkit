{
  "name": "sfpowerkit",
  "description": "Salesforce DevOps Helper Extensions to use in CI/CD Pipelines",
  "version": "1.9.0",
  "author": "Azlam Abdulsalam @azlamsalam",
  "bugs": "https://github.com/azlamsalam/sfpowerkit/issues",
  "dependencies": {
    "@oclif/command": "1",
    "@oclif/config": "1",
    "@oclif/errors": "1",
    "@salesforce/command": "^1.4.1",
    "@salesforce/core": "^1.3.2",
    "child-process": "^1.0.2",
    "child-process-promise": "^2.2.1",
    "cli-ux": "^5.2.1",
    "decompress": "^4.2.0",
    "fs-extra": "^7.0.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "tslib": "1",
    "xml2js": "^0.4.19",
    "unzipper":"0.9.11"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "^1.4.1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^11",
    "chai": "^4",
    "globby": "^9",
    "mocha": "^6",
    "nyc": "^13",
    "rimraf": "^2.6.3",
    "sinon": "7",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/azlamsalam/sfpowerkit",
  "keywords": [
    "sfdx-plugin",
    "sfdx",
    "plugin",
    "salesforce",
    "cli"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "topics": {
      "sfpowerkit:package": {
        "description": "Helper commands for unlocked package"
      },
      "sfpowerkit:org": {
        "description": "Helper commands for org such as connectedapp, creation of sandbox etc"
      },
      "sfpowerkit:custommetadata": {
        "description": "Import CustomMetadata from Excel"
      },
      "sfpowerkit":{
        "description": "Salesforce DevOps Helper Extensions to use in CI/CD Pipelines"
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "azlamsalam/sfpowerkit",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tslint -p test -t stylish",
    "prepare": "rimraf lib && tsc -b && oclif-dev manifest && npm shrinkwrap",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "git add README.md"
  }
}
